#%RAML 1.0 Library
annotationTypes:
  MyFavoriteAnnotation: string
types:
  Url:
    minLength: 2
    pattern: dddd
    example: "dddd"
    (MyFavoriteAnnotation): h
    facets:
      link?: string
  Person:
    description: Single Person
    properties:
      name: string
      lastName: string
  Group:
    description: Group of People
    properties:
      name:
        type: string
        description: |
          uniquie identifier of the property
        minLength: 4
        maxLength: 5
        default: "dddd"
        enum: [dddd, ddd2]
        (MyFavoriteAnnotation): h
      members?: Person[]
  Q:
      properties:
        m: number
        q?: Q
        x:
          description: this is person
          type: Person
          xml:
              name: s
              attribute: true
      xml:
        name: s
        attribute: true
  BigGroup:
    type: [ Person, Group]
    additionalProperties: true
    properties:
      subGroups: (Group | BigGroup [] | number)[]
      /ee/: number
      fff:
        properties:
          x: number
          y: number
          mmm:
            properties:
              zzz: number
      another:
        type: array
        items:
          properties:
            d: Url
            x:
              description: this is person
              type: Person
            y: Person
    patternProperties:
      'dde': string
